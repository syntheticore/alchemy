{
  "name": "alchemy-web",
  "version": "0.1.0",
  "productName": "Alchemy",
  "description": "Parametric CAD",
  "author": "Bj√∂rn Breitgoff <syntheticore@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && webpack --mode production --env.production --env.wc",
    "build-electron": "npm run clean && webpack --mode production --env.production",
    "serve": "cd dist && live-server --port=8080 --open=/dev.html",
    "serve-dev": "npm run clean && webpack-dev-server --env.development --hot --open -d",
    "electron": "npm run build-electron && electron ./dist",
    "electron-pack": "npm run build-electron && electron-packager ./dist --overwrite",
    "test": "cargo test && wasm-pack test --headless",
    "clean": "rimraf dist rust/pkg",
    "real-clean": "npm run clean && rimraf node_modules rust/**/target"
  },
  "build": {
    "appId": "com.alchemy.electron"
  },
  "devDependencies": {
    "@vue/web-component-wrapper": "^1.2.0",
    "@wasm-tool/wasm-pack-plugin": "^0.4.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.4.2",
    "electron": "^7.1.11",
    "electron-packager": "^14.2.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nib": "^1.1.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "three": "^0.113.2",
    "vue": "^2.6.11"
  }
}
