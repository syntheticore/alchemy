{
  "name": "alchemy-web",
  "version": "0.1.0",
  "productName": "Alchemy",
  "description": "Parametric CAD",
  "author": "Bj√∂rn Breitgoff <syntheticore@gmail.com>",
  "main": "index.js",
  "sideEffects": true,
  "scripts": {
    "build": "npm run clean && webpack --mode production --env.production --env.wc",
    "build-electron": "npm run clean && webpack --mode production --env.production",
    "start": "cd dist && live-server --port=8080 --open=/dev.html",
    "dev": "npm run clean && webpack-dev-server --env.development --hot --open -d",
    "dev-electron": "npm run build-electron && electron ./dist",
    "pack-electron": "npm run build-electron && electron-packager ./dist --overwrite --helperBundleId=com.alchemy.electron",
    "package-mac": "electron-packager ./dist --overwrite --platform=darwin --arch=x64 --icon=./alchemy.icns --prune=true --out=release-builds",
    "package-win": "electron-packager ./dist electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./alchemy.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CryptoApp\"",
    "package-linux": "electron-packager ./dist --overwrite --asar=true --platform=linux --arch=x64 --icon=./alchemy1024x1024.png --prune=true --out=release-builds",
    "test": "cargo test && wasm-pack test --headless",
    "clean": "rimraf dist rust/pkg",
    "real-clean": "npm run clean && rimraf node_modules rust/**/target"
  },
  "build": {
    "appId": "com.alchemy.electron"
  },
  "devDependencies": {
    "@vue/web-component-wrapper": "^1.2.0",
    "@wasm-tool/wasm-pack-plugin": "^0.4.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.4.2",
    "electron": "^8.0.0",
    "electron-packager": "^14.2.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nib": "^1.1.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1",
    "nanoevents": "^5.1.8",
    "three": "^0.113.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "vue": "^2.6.11"
  }
}
